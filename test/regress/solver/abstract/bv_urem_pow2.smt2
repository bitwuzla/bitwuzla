(set-info :status unsat)
(set-option :abstraction true)
(set-option :abstraction-bv-size 16)
(set-option :time-limit-per 1000)
(declare-const a (_ BitVec 16))
(declare-const b (_ BitVec 16))
(declare-const c (_ BitVec 16))
(declare-const pow2 (_ BitVec 16))
(declare-const mask (_ BitVec 16))
(declare-const shift_by (_ BitVec 16))
(assert (bvult shift_by (_ bv16 16)))
(assert (= pow2 (bvshl (_ bv1 16) shift_by)))
(assert (= mask (bvnot (bvshl (bvnot (_ bv0 16)) shift_by))))
(assert (= c (bvurem a pow2)))
(assert (distinct c (bvand mask a)))
(check-sat)
