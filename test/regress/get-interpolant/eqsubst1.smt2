; ignore output
(set-option :produce-interpolants true)
(set-option :interpolants-subst true)
(declare-const x1 (_ BitVec 8))
(declare-const x2 (_ BitVec 8))
(declare-const x3 (_ BitVec 8))
(assert (! (and (bvslt (_ bv0 32) (bvsub (concat (_ bv0 24) x1) (_ bv1 32))) (= x2 x1)) :named a1))
(assert (and (= x3 ((_ extract 7 0) (bvneg (concat (_ bv0 24) x2)))) (= x3 (_ bv0 8))))
(set-info :status unsat)
(check-sat)
(get-interpolant (a1))
