# Disable wine debug output if we run the cross-compiled binary
env = []
if meson.is_cross_build() and host_machine.system() == 'windows'
  env += ['WINEDEBUG=fixme-all']
endif

foreach t : regress_tests
  tests = t[0]
  run_regress_script_options = []
  global_args = []
  if t.length() >= 2
    run_regress_script_options += t[1]
  endif
  if t.length() == 3
    global_args += t[2]
  endif

  foreach test_args : tests
    file = test_args[0]
    name = file.replace('/', '_')
    test_case = [join_paths(meson.current_source_dir(), file)]
    test_case += global_args
    if test_args.length() == 2
      test_case += test_args[1]
    endif
    args = [run_regress_script.full_path()]
    args += bitwuzla_bin.full_path()
    args += ' '.join(test_case)
    args += meson.current_build_dir()
    args += run_regress_script_options
    test(name, python, args: args, suite: 'regress', depends: bitwuzla_bin,
         timeout: 120, env: env)
  endforeach
endforeach
