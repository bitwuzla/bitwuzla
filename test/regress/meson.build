  [tests_smt2],
  [tests_smt2, [], ['--produce-unsat-cores', '--check-unsat-core']],
  [tests_btor2_parser],
  [tests_btor2_sat, '--check-sat'],
  [tests_btor2_unsat, '--check-unsat'],
  [tests_btor2_unsat, '--check-unsat', ['--produce-unsat-cores', '--check-unsat-core']],
